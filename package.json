{
  "name": "@port-labs/port-sdk",
  "version": "0.1.0",
  "description": "Type-safe TypeScript SDK for Port.io API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "generate-types": "openapi-typescript openapi.json -o src/types/api.ts",
    "types:check": "tsx scripts/check-types.ts",
    "types:validate": "pnpm type-check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "prepublishOnly": "pnpm types:check && pnpm run type-check && pnpm run test && pnpm run build",
    "precommit": "pnpm run type-check && pnpm run test",
    "example": "tsx examples/basic-usage.ts"
  },
  "keywords": [
    "port",
    "port.io",
    "internal-developer-portal",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "openapi-typescript": "^7.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "undici": "^7.16.0"
  }
}
